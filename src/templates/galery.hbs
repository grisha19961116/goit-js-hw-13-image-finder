{{#each this}}
<li class="menu__item">
<div class="photo-card">
  <img src="{{this.webformatURL}}" alt="{{this.user}}" class="img-hbs" />

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{this.likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{this.views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{this.comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{this.downloads}}
    </p>
  </div>
</div>
</li>
{{/each}}

