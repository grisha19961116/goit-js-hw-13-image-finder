{{#each this}}
<li class="gallery__item">
  <div class="photo-card">
    <img src="{{this.webformatURL}}" alt="{{this.user}}" class="gallery__img" data-set="{{this.largeImageURL}}" />

    <div class="stats">
      <p class="stats-item">
        <i class="material-icons like">thumb_up</i>
        {{this.likes}}
      </p>
      <p class="stats-item">
        <i class="material-icons view">visibility</i>
        {{this.views}}
      </p>
      <p class="stats-item">
        <i class="material-icons comment">comment</i>
        {{this.comments}}
      </p>
      <p class="stats-item">
        <i class="material-icons icons">cloud_download</i>
        {{this.downloads}}
      </p>
    </div>
  </div>
</li>
{{/each}}